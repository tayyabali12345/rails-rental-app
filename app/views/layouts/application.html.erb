<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>RENTER APP</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <body>
    <%# <%= render 'dashboard/header' %>

    <div class="container">
      <nav class='navbar navbar-secondary bg-light'>
              <ul class="nav navbar-nav navbar-right">
                <% if user_signed_in? %>
                  <div class="row">
                    <div class="d-flex justify-content-end">
                      <%= link_to 'logout',  destroy_user_session_path, :class => 'btn btn-danger', method: :delete %>
                      <%# <%= link_to 'Edit Profile',  edit_user_registration_path, :class => 'btn btn-warning'%>
                    </div>
                  </div>
                <%else%>
                  <div class="row">
                    <div class="d-flex justify-content-end">
                      <%= link_to 'Login',  new_user_session_path, :class => 'btn btn-danger' %>

                    </div>
                  </div>
                <% end %>
              </ul>
      </nav>

      <h3 class="text-center">Welcome User</h3>
      <%= yield %>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>
  </body>
</html>
